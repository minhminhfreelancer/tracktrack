{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://tracktrack.pages.dev/api/:path*"
    },
    {
      "source": "/tracker.js",
      "destination": "https://tracktrack.pages.dev/tracker.js"
    }
  ]
}